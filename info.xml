<?xml version="1.0"?>
<extension key="org.civicrm.angularhookpolyfill" type="module">
  <file>angularhookpolyfill</file>
  <name>Angular Hook Polyfill</name>
  <description>This extension adds the crmHook service to the civicrm angular app, so that hooks can be utilized within angular pages</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Tobias Lounsbury</author>
    <email>tobiaslounsbury@gmail.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://https://github.com/TobiasLounsbury/crmHookPolyfill</url>
    <url desc="Documentation">http://https://github.com/TobiasLounsbury/crmHookPolyfill</url>
    <url desc="Support">http://https://github.com/TobiasLounsbury/crmHookPolyfill/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2016-05-23</releaseDate>
  <version>2.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.6</ver>
    <ver>4.7</ver>
  </compatibility>
  <comments>Angular crmHook Polyfill Extension</comments>
  <civix>
    <namespace>CRM/Angularhookpolyfill</namespace>
  </civix>
</extension>
